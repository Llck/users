<%= link_to 'New client', new_user_path %>
<table border="1">
  <tr>
    <th>Name</th>
    <th>Email</th>
    <thcosplan = "3"></th?
    </tr>

    <tbody>
      <% @client.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>

        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Delete', user, :method :delete %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
